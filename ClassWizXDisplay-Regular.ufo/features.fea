languagesystem DFLT dflt;
languagesystem latn dflt;
languagesystem cyrl dflt;
languagesystem hani dflt;
languagesystem kana dflt;
#< languagesystem

lookup CALTH_SUB {
  sub c by Calth-gk;
  sub a by cAlth-gk;
  sub h by caltH-gk;
} CALTH_SUB;

lookup CALTH_LIGS {
  sub l t by caLTh-gk;
  sub slash parenleft by calth-gk;
  sub g asterisk by calth-Gk;
  sub K parenright by calth-gK;
} CALTH_LIGS;

lookup KGK_SUB {
  sub k by Kgk;
} KGK_SUB;

lookup KGK_LIGS {
  sub slash parenleft by kgk;
  sub g asterisk by kGk;
  sub K parenright by kgK;
} KGK_LIGS;
